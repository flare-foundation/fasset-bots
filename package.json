{
  "name": "fasset-liquidator",
  "version": "0.0.0",
  "description": "Liquidator utilities for the f-asset bridge on the Flare network",
  "scripts": {
    "clean": "rimraf ./artifacts ./cache ./coverage ./types ./coverage.json && yarn typechain",
    "compile": "rimraf typechain && yarn hardhat compile",
    "fork": "yarn hardhat node --fork https://coston-api.flare.network/ext/C/rpc --fork-block-number 10556413",
    "test": "yarn hardhat test",
    "fix-blazeswap-hash": "yarn hardhat compile && ./scripts/replace-blazeswap-hash.sh && yarn compile"
  },
  "license": "MIT",
  "devDependencies": {
    "@ethersproject/keccak256": "^5.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.3.6",
    "@types/dotenv": "^8.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "bn.js": "^5.2.1",
    "chai": "^4.3.10",
    "hardhat": "^2.17.3",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "blazeswap": "git+https://github.com/blazeswap/contracts.git#main",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
    "fasset": "git+ssh://git@gitlab.com/flarenetwork/fasset#fasset-tests"
  }
}
