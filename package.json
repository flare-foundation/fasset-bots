{
  "name": "fasset-liquidator",
  "version": "0.0.0",
  "description": "Liquidator bot for the f-asset bridge on the Flare network",
  "scripts": {
    "compile": "yarn replace-blazeswap-hash; rimraf typechain-types typechain-truffle && yarn hardhat compile && yarn typechain-truffle-v5 && yarn typechain-ethers-v6",
    "typechain-ethers-v6": "yarn typechain --target=ethers-v6 --out-dir typechain-ethers \"artifacts/!(build-info|flattened)/**/+([a-zA-Z0-9_]).json\"",
    "typechain-truffle-v5": "yarn typechain --target=truffle-v5 --out-dir typechain-truffle \"artifacts/!(build-info|flattened)/**/+([a-zA-Z0-9_]).json\"",
    "replace-blazeswap-hash": "./scripts/replace-blazeswap-hash.sh",
    "test": "yarn hardhat test"
  },
  "license": "MIT",
  "devDependencies": {
    "@ethersproject/keccak256": "^5.7.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^8.0.3",
    "@typechain/truffle-v5": "^8.0.6",
    "@typechain/web3-v1": "^6.0.6",
    "@types/dotenv": "^8.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "hardhat": "^2.17.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "blazeswap": "git+https://github.com/blazeswap/contracts.git#main",
    "bn.js": "^5.2.1",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
    "fasset": "git+ssh://git@gitlab.com/flarenetwork/fasset#fasset-tests",
    "flare-smart-contracts": "git+https://gitlab.com/flarenetwork/flare-smart-contracts.git#flare-distribution-to-delegators"
  }
}
