{
    "private": true,
    "workspaces": {
        "packages": [
            "packages/*"
        ]
    },
    "engines": {
        "node": ">=16.10.0"
    },
    "scripts": {
        "---------BUILD---SCRIPTS": "",
        "clean": "rm -rf dist packages/*/dist",
        "clean-nm": "rm -rf node_modules && rm -rf packages/*/node_modules",
        "build": "tsc --build",
        "watch": "tsc --build --watch --incremental",
        "update-references": "utils-typescript-references",
        "---------LINT---SCRIPTS": "",
        "lint": "eslint \"packages/**/src/**/*ts\"",
        "---------PRETTIER---SCRIPTS": "",
        "prettier_check": "prettier --check .",
        "prettier_fix": "prettier --write ."
    },
    "devDependencies": {
        "@goldstack/utils-typescript-references": "0.3.9",
        "@istanbuljs/nyc-config-typescript": "1.0.2",
        "@types/chai": "4.3.6",
        "@types/chai-as-promised": "7.1.6",
        "@types/chai-spies": "1.0.4",
        "@typescript-eslint/eslint-plugin": "6.7.3",
        "@typescript-eslint/parser": "6.7.3",
        "chai": "4.3.8",
        "chai-as-promised": "7.1.1",
        "chai-spies": "1.0.0",
        "dotenv": "16.3.1",
        "eslint": "8.50.0",
        "eslint-plugin-node": "11.1.0",
        "mocha": "10.2.0",
        "nyc": "15.1.0",
        "prettier": "3.0.3",
        "rewire": "7.0.0",
        "rimraf": "5.0.5",
        "source-map-support": "0.5.21",
        "ts-node": "10.9.1",
        "typescript": "5.2.2"
    },
    "packageManager": "yarn@3.6.4"
}
