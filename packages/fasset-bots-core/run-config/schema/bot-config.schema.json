{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false,
    "definitions": {
        "BotFAssetInfo": {
            "additionalProperties": false,
            "properties": {
                "chainId": {
                    "type": "string"
                },
                "inTestnet": {
                    "type": "boolean"
                },
                "indexerUrl": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "priceChangeEmitter": {
                    "type": "string"
                },
                "symbol": {
                    "type": "string"
                },
                "walletUrl": {
                    "type": "string"
                }
            },
            "required": [
                "chainId",
                "name",
                "symbol"
            ],
            "type": "object"
        },
        "BotStrategyDefinition": {
            "additionalProperties": false,
            "properties": {
                "className": {
                    "type": "string"
                },
                "config": {}
            },
            "required": [
                "className"
            ],
            "type": "object"
        },
        "DatabaseType": {
            "enum": [
                "mysql",
                "postgresql",
                "sqlite"
            ],
            "type": "string"
        },
        "NativeChainInfo": {
            "additionalProperties": false,
            "properties": {
                "finalizationBlocks": {
                    "type": "number"
                },
                "readLogsChunkSize": {
                    "type": "number"
                }
            },
            "required": [
                "finalizationBlocks",
                "readLogsChunkSize"
            ],
            "type": "object"
        },
        "OrmConfigOptions": {
            "additionalProperties": {},
            "properties": {
                "charset": {
                    "type": "string"
                },
                "clientUrl": {
                    "type": "string"
                },
                "dbName": {
                    "type": "string"
                },
                "debug": {
                    "type": "boolean"
                },
                "host": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "port": {
                    "type": "number"
                },
                "schemaUpdate": {
                    "enum": [
                        "full",
                        "none",
                        "recreate",
                        "safe"
                    ],
                    "type": "string"
                },
                "type": {
                    "$ref": "#/definitions/DatabaseType"
                },
                "user": {
                    "type": "string"
                }
            },
            "required": [
                "type"
            ],
            "type": "object"
        }
    },
    "properties": {
        "$schema": {
            "type": "string"
        },
        "alertsUrl": {
            "type": "string"
        },
        "assetManagerController": {
            "type": "string"
        },
        "attestationProviderUrls": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "challengeStrategy": {
            "$ref": "#/definitions/BotStrategyDefinition"
        },
        "contractsJsonFile": {
            "type": "string"
        },
        "fAssets": {
            "additionalProperties": {
                "$ref": "#/definitions/BotFAssetInfo"
            },
            "type": "object"
        },
        "liquidationStrategy": {
            "$ref": "#/definitions/BotStrategyDefinition"
        },
        "loopDelay": {
            "type": "number"
        },
        "nativeChainInfo": {
            "$ref": "#/definitions/NativeChainInfo"
        },
        "ormOptions": {
            "$ref": "#/definitions/OrmConfigOptions"
        },
        "prioritizeAddressUpdater": {
            "type": "boolean"
        },
        "rpcUrl": {
            "type": "string"
        },
        "walletOptions": {
            "additionalProperties": false,
            "properties": {
                "blockOffset": {
                    "type": "number"
                },
                "feeIncrease": {
                    "type": "number"
                },
                "retries": {
                    "type": "number"
                }
            },
            "type": "object"
        }
    },
    "required": [
        "fAssets",
        "loopDelay",
        "nativeChainInfo",
        "prioritizeAddressUpdater",
        "rpcUrl"
    ],
    "type": "object"
}

