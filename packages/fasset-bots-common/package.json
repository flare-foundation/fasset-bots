{
    "name": "@flarenetwork/fasset-bots-common",
    "version": "1.3.3",
    "description": "FAsset bots library",
    "author": "Flare Foundation",
    "homepage": "https://github.com/flare-foundation/fasset-bots",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flare-foundation/fasset-bots.git",
        "directory": "packages/fasset-bots-core"
    },
    "license": "MIT",
    "engines": {
        "node": ">=18.12.0"
    },
    "exports": {
        ".": {
