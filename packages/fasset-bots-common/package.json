        ]
    },
    "files": [
        "/dist",
        "/src"
    ],
    "dependencies": {
        "axios": "1.5.1",
        "axios-retry": "3.8.0",
        "chalk": "4.1.2",
        "dotenv": "16.3.1",
        "glob": "10.3.10",
        "source-map-support": "0.5.21",
        "winston": "3.10.0",
        "winston-daily-rotate-file": "4.7.1",
        "winston-transport": "4.5.0"
    },
    "devDependencies": {
        "@types/chai": "4.3.6",
        "chai": "4.5.0",
        "typescript": "5.8.3",
        "typescript-json-schema": "0.61.0"
    },
    "packageManager": "yarn@3.6.4"
